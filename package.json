{
  "name": "next-boilerplate",
  "version": "0.1.0",
  "author": "Gustavo Silva (http://github.com/gusilva)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts --fix",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc './**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc './**/*.ts' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"./**/*.ts\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange './**/*.ts' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange './**/*.ts' -- prettier --write {{changed}}",
    "test": "jest",
    "test:dev": "jest --watchAll"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.5.3",
    "next": "10.0.5",
    "next-connect": "^0.9.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "ts-jest": "^26.4.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "^1.19.1",
    "run-script-os": "^1.1.4",
    "supertest": "^4.0.2",
    "typescript": "^4.1.3"
  }
}
